{
	"title" : "shipping-method",
	"version" : 0,
	"primaryKey" : "_id",
	"type" : "object",
	"required" : [
		"_deleted",
		"_rev",
		"_meta",
		"_attachments",
		"title",
		"description",
		"type",
		"slug"
	],
	"properties" : {
		"id" : {
			"type" : "number"
		},
		"_id" : {
			"type" : "string"
		},
		"title" : {
			"type" : "string"
		},
		"description" : {
			"type" : "string"
		},
		"type" : {
			"type" : "string",
			"enum" : [
				"flat_rate",
				"free_shipping",
				"local_pickup"
			]
		},
		"slug" : {
			"type" : "string"
		},
		"_deleted" : {
			"type" : "boolean"
		},
		"_rev" : {
			"type" : "string",
			"minLength" : 1
		},
		"_meta" : {
			"type" : "object",
			"required" : [
				"lwt"
			],
			"properties" : {
				"lwt" : {
					"type" : "number",
					"minimum" : 1,
					"maximum" : 1000000000000000,
					"multipleOf" : 0.01
				}
			}
		},
		"_attachments" : {
			"type" : "object"
		}
	}
}
